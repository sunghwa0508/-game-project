      "cell_type": "code",
      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "\n",
        "# 크롤링할 스팀 게임의 리뷰 페이지 URL을 지정합니다.\n",
        "steam_review_url = 'https://store.steampowered.com/app/1868140/reviews/?p=1&browsefilter=toprated'\n",
        "\n",
        "# 스팀 리뷰 페이지의 HTML을 가져옵니다.\n",
        "response = requests.get(steam_review_url)\n",
        "if response.status_code == 200:\n",
        "    # HTML을 BeautifulSoup를 사용하여 파싱합니다.\n",
        "    soup = BeautifulSoup(response.text, 'html.parser')\n",
        "\n",
        "    # 리뷰 항목을 찾습니다. 리뷰 항목은 주로 div 태그와 class 속성을 사용하여 식별됩니다.\n",
        "    review_items = soup.find_all('div', class_='apphub_Card')\n",
        "\n",
        "    for review_item in review_items:\n",
        "        # 리뷰 내용, 작성자, 등급 등을 추출합니다.\n",
        "        review_text = review_item.find('div', class_='apphub_CardTextContent').text.strip()\n",
        "        reviewer = review_item.find('div', class_='apphub_CardContentAuthorName').text.strip()\n",
        "        rating = review_item.find('div', class_='apphub_CardContentMoreLink').text.strip()\n",
        "\n",
        "        # 추출된 정보를 출력하거나 저장합니다.\n",
        "        print(f'Reviewer: {reviewer}')\n",
        "        print(f'Rating: {rating}')\n",
        "        print(f'Review: {review_text}')\n",
        "        print('---')\n",
        "\n",
        "else:\n",
        "    print('Failed to retrieve the Steam review page.')"
      ],
      "metadata": {
        "id": "ZWhauy2HNI3b"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "\n",
        "# 크롤링할 스팀 게임의 리뷰 페이지 URL을 지정합니다.\n",
        "steam_review_url = 'https://store.steampowered.com/app/1868140/reviews/?p=1&browsefilter=toprated'\n",
        "\n",
        "# 스팀 리뷰 페이지의 HTML을 가져옵니다.\n",
        "response = requests.get(steam_review_url)\n",
        "if response.status_code == 200:\n",
        "    # HTML을 BeautifulSoup를 사용하여 파싱합니다.\n",
        "    soup = BeautifulSoup(response.text, 'html.parser')\n",
        "\n",
        "    # 리뷰 항목을 찾습니다. 리뷰 항목은 주로 div 태그와 class 속성을 사용하여 식별됩니다.\n",
        "    review_items = soup.find_all('div', class_='apphub_Card')\n",
        "\n",
        "    for review_item in review_items:\n",
        "        # 리뷰 내용, 작성자, 등급 등을 추출합니다.\n",
        "        review_text = review_item.find('div', class_='apphub_CardTextContent').text.strip()\n",
        "        reviewer = review_item.find('div', class_='apphub_CardContentAuthorName').text.strip()\n",
        "        rating = review_item.find('div', class_='apphub_CardContentMoreLink').text.strip()\n",
        "\n",
        "        # 추출된 정보를 출력합니다.\n",
        "        print(f'Reviewer: {reviewer}')\n",
        "        print(f'Rating: {rating}')\n",
        "        print(f'Review: {review_text}')\n",
        "        print('---')\n",
        "\n",
        "else:\n",
        "    print('Failed to retrieve the Steam review page.')\n",
        "#위 코드를 코드 셀에 붙여넣고 실행하면 리뷰 정보가 출력됩니다. 출력은 코드 셀 아래에 텍스트 형태로 표시됩니다."
      ],
      "metadata": {
        "id": "DYZXvIdWS4zN"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "import time"
      ],
      "metadata": {
        "id": "nh0BKziGThXQ"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def get_reviews(appid, params={'json':1}):\n",
        "        url = 'https://store.steampowered.com/appreviews/'\n",
        "        response = requests.get(url=url+str(appid), params=params, headers={'User-Agent': 'Mozilla/5.0'})\n",
        "        return response.json()\n",
        "\n",
        "def get_n_reviews(appid, n=100):\n",
        "    reviews = []\n",
        "    cursor = '*'\n",
        "    params = {\n",
        "        'json' : 1,\n",
        "        'filter' : 'all',\n",
        "        'language' : 'all',\n",
        "        'day_range' : 9223372036854775807,\n",
        "        'review_type' : 'all',\n",
        "        'purchase_type' : 'all'\n",
        "    }\n",
        "\n",
        "    while n > 0:\n",
        "        time.sleep(2)\n",
        "        params['cursor'] = cursor.encode()\n",
        "        params['num_per_page'] = min(100, n)\n",
        "        n -= 100\n",
        "\n",
        "        response = get_reviews(appid, params)\n",
        "        cursor = response['cursor']\n",
        "        reviews += response['reviews']\n",
        "\n",
        "        if len(response['reviews']) < 100: break\n",
        "\n",
        "    return reviews"
      ],
      "metadata": {
        "id": "9LuHdIW2TZu7"
      },
      "execution_count": 29,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "reviews = get_n_reviews(1868140, 100000)"
      ],
      "metadata": {
        "id": "LV45ztHKTdjo"
      },
      "execution_count": 30,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
