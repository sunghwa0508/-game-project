        "from collections import Counter\n",
        "\n",
        "count = Counter(nouns)\n",
        "words = dict(count.most_common())\n",
        "\n",
        "for i, (word, count) in enumerate(words.items()):\n",
        "    if i > 20:\n",
        "        break\n",
        "\n",
        "    print(word, count)"
