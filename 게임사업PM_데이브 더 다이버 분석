        "from konlpy.tag import Komoran, Okt, Kkma, Hannanum\n",
        "from tqdm import tqdm\n",
        "\n",
        "extractor = Hannanum()\n",
        "\n",
        "nouns = []\n",
        "\n",
        "for review in tqdm(df_kor['review'].values.tolist()):\n",
        "    nouns.extend(extractor.nouns(review))\n",
        "\n",
        "len(nouns)"
      ],
